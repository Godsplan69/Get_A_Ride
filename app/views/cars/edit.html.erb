<h1>EDIT</h1>
<%= form_for(@car) do |f| %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :price_per_day %>
  <%= f.text_field :price_per_day %>
  <%= f.label :immatriculation %>
  <%= f.text_field :immatriculation %>
  <%= f.label :localisation %>
  <%= f.text_field :localisation %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.submit %>
<% end %>
